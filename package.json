{
  "name": "violet-infrastructure",
  "version": "1.0.0",
  "private": true,
  "license": "TODO",
  "scripts": {
    "compile": "pnpm run --filter=./pkg/shared compile && pnpm run --filter=./pkg --filter=./pkg/shared",
    "cdktf-prepare:manager": "run-p cdktf-prepare:_:*",
    "cdktf-prepare:env": "pnpm run cdktf-prepare:_:compile-bot",
    "cdktf-prepare:_:compile-bot": "pnpm run --filter=./pkg/bot compile",
    "cdktf-prepare:_:zip-self": "ts-node -T ./pkg/shared/scripts/local/zip-self.ts",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts,tsx,json}\"",
    "lint-fix": "pnpm run lint -- --fix",
    "lint-quiet": "pnpm run lint -- --quiet",
    "test": "pnpm run --filter=./pkg test",
    "test-watch": "pnpm run --filter=./pkg test-watch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^2.0.13",
    "@cdktf/provider-null": "^0.2.126",
    "@cdktf/provider-random": "^0.0.5",
    "@luma-dev/eslint-config-base": "^0.8.0",
    "@luma-dev/eslint-config-jest": "^0.2.6",
    "@luma-dev/prettier-config": "^0.1.0",
    "@self/bot": "workspace:*",
    "@self/def-env": "workspace:*",
    "@self/def-manager": "workspace:*",
    "@self/shared": "workspace:*",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/promise-queue": "^2.2.0",
    "arg": "^5.0.1",
    "cdktf": "0.7.0",
    "cdktf-cli": "^0.7.0",
    "constructs": "^10.0.9",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.13",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^7.32.0",
    "fast-glob": "^3.2.7",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "promise-queue": "^2.2.5",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "zod": "^3.11.6"
  },
  "engines": {
    "node": ">=14",
    "npm": "use pnpm",
    "pnpm": ">=6",
    "yarn": "use pnpm"
  }
}
