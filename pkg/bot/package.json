{
  "name": "@self/bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "ts-node -T ./node_modules/@self/shared/scripts/local/build-files.ts --from-dir ./src/entry/lambda/ --to-dir build --target node14",
    "compile-watch": "pnpm run compile -- --watch",
    "test": "run-s test:tsc test:jest",
    "test-watch": "run-p test-watch:*",
    "test-watch:jest": "jest --watch",
    "test-watch:tsc": "tsc --noEmit --watch",
    "test:jest": "jest",
    "test:tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.48.0",
    "@aws-sdk/client-codebuild": "^3.48.0",
    "@aws-sdk/client-dynamodb": "^3.48.0",
    "@aws-sdk/client-ecr": "^3.48.0",
    "@aws-sdk/credential-providers": "^3.48.0",
    "@aws-sdk/types": "^3.47.1",
    "@aws-sdk/util-dynamodb": "^3.48.0",
    "@js-temporal/polyfill": "^0.3.0",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/rest": "^18.12.0",
    "@octokit/webhooks": "^9.22.0",
    "@self/shared": "workspace:^1.0.0",
    "@types/aws-lambda": "^8.10.91",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "chokidar": "^3.5.3",
    "dotenv": "^14.2.0",
    "extract-zip": "^2.0.1",
    "node-fetch": "2",
    "pretty-bytes": "^5.6.0",
    "safe-stable-stringify": "^2.3.1",
    "ts-expect": "^1.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "winston-cloudwatch": "^3.1.1"
  },
  "engines": {
    "node": ">=14",
    "npm": "use pnpm",
    "pnpm": ">=6",
    "yarn": "use pnpm"
  }
}
